{
  "name": "officematch",
  "version": "1.0.0",
  "description": "Office Show - Love Match",
  "main": "app.js",
  "scripts": {
    "build-ts": "tsc",
    "lint": "tslint -c test/tslint.json 'src/*.ts'",
